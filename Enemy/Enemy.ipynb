    "using System;\n",
    "\n",
    "public abstract class Enemy\n",
    "{\n",
    "\tpublic int Health { get; protected set; }\n",
    "\tpublic int Damage { get; protected set; }\n",
    "\n",
    "\tpublic void TakeDamage(int amount)\n",
    "\t{\n",
    "\t\tHealth -= amount;\n",
    "\t\tif (Health < 0) Health = 0;\n",
    "\t}\n",
    "\n",
    "}"
   ]
